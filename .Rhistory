ggplot(data = plot.data, aes(x = period, y =net_cost_total/1000, colour = section)) +
geom_point(fill="black") + geom_line() + expand_limits(y=0) +
xlab("Time") + ylab("NET COST (£000)") + theme_bw() + theme(legend.position="top")
}
